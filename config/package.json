{
  "name": "adrian-sparc-project",
  "version": "1.0.0",
  "description": "SPARC development environment with Claude Flow orchestration",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "sparc:modes": "npx claude-flow sparc modes",
    "sparc:info": "npx claude-flow sparc info",
    "sparc:tdd": "npx claude-flow sparc tdd",
    "sparc:pipeline": "npx claude-flow sparc pipeline"
  },
  "keywords": ["sparc", "claude-flow", "tdd", "orchestration"],
  "author": "Adrian",
  "license": "MIT",
  "dependencies": {
    "claude-flow": "alpha"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}